{
  "scripts": {
    "clean": "dotnet fable clean --yes && rm -rf dist",
    "install": "dotnet tool restore",
    "start": "dotnet fable watch src --sourceMaps --run vite --clearScreen=false",
    "build": "dotnet fable src && vite build --emptyOutDir",
    "preview": "dotnet fable src && vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "vite": "^3.0.4"
  },
  "dependencies": {
    "lit": "^2.2.8",
    "webrtc-adapter": "^8.1.1"
  }
}
